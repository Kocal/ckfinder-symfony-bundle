includes:
    - ./phpstan-baseline.neon
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - vendor/phpstan/phpstan-phpunit/rules.neon

parameters:
    level: max

    paths:
        - src
        - tests

    excludePaths:
        - src/_connector

    bootstrapFiles:
        - vendor/bin/.phpunit/phpunit/vendor/autoload.php